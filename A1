#include <iostream>
#include <string>
#include <vector>

using std::vector;
using std::cout;
using std::cin;
using std::string;

int main() {
    std::ios_base::sync_with_stdio(false);
    std::cin.tie(nullptr);
    std::cout.tie(nullptr);

    vector <string> commands;
    vector <int> values;
    vector <int> answers;

    string s;
    while (cin >> s) {

        if (s == "exit") {
            break;
        } 
        if (s == "push") {
            int n;
            cin >> n;
            commands.push_back(s);
            values.push_back(n);
        } else {
            commands.push_back(s);
            values.push_back(0);
        }

    }
    
    for (int i = 0; i < commands.size(); i++) {
        if (commands[i] == "push") {
            answers.push_back(values[i]);
            cout << "ok" << '\n';
        }
        if (commands[i] == "pop") {
            if (answers.empty()) {
                cout << "error" << '\n';
            } else {
                cout << answers[answers.size() - 1] << '\n';
                answers.pop_back();
            }
        }
        if (commands[i] == "back") {
            if (answers.empty()) {
                cout << "error" << '\n';
            } else {
                cout << answers[answers.size() - 1] << '\n';
            }
        } 
        if (commands[i] == "size") {
            cout << answers.size() << '\n';
        } 
        if (commands[i] == "clear") {
            answers.clear();
            cout << "ok" << '\n';
        }
    }
    cout << "bye";
}

